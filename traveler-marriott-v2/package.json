{
  "private": true,
  "scripts": {
    "build": "npm run build:legacy && node node_modules/cross-env/dist/bin/cross-env.js SKIP_CLEAN=true npm run build:modern",
    "build:legacy": "node node_modules/cross-env/dist/bin/cross-env.js BUILD_MODERN=false npm run build:base",
    "build:modern": "node node_modules/cross-env/dist/bin/cross-env.js BUILD_MODERN=true npm run build:base",
    "build:base": "node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watching": "npm run watching:legacy && node node_modules/cross-env/dist/bin/cross-env.js SKIP_CLEAN=true npm run watching:modern",
    "watching:legacy": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development BUILD_MODERN=false npm run build:base",
    "watching:modern": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development BUILD_MODERN=true npm run watching:base",
    "watching:base": "node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --env.NODE_ENV=development --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run development",
    "development": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development BUILD_ENV=development npm run build",
    "stag": "npm run staging",
    "staging": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production BUILD_ENV=staging npm run build",
    "prod": "npm run production",
    "production": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production BUILD_ENV=production npm run build",
    "watcher": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development BUILD_ENV=development BUILD_MODERN=true npm run watching",
    "lint:js": "eslint \"./resources/src/**/*.{js,vue}\"",
    "lint:sass": "stylelint \"./resources/src/**/*.{scss,sass}\"",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint-fix": "prettier \"./resources/src/**/*.{js,vue}\" --write && npm run lint:js -- --fix",
    "amp": "cross-env NODE_ENV=production BUILD_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=amp-webpack.mix.js",
    "amp:watch": "cross-env NODE_ENV=production BUILD_ENV=production node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --env.mixfile=amp-webpack.mix.js"
  },
